			<!-- Modal Usr Nuevo-->
			<div class="modal fade" id="usuarioMDL" role="dialog">
				<div class="modal-dialog">
					<div class="modal-content">
					    <div class="modal-header">
					        <button type="button" class="close" data-dismiss="modal">&times;</button>
					        <h4><span class="glyphicon glyphicon-list-alt"></span> Nuevo Usuario </h4>
					    </div>
					    <div class="modal-body">
					   		<!-- <form action="" onSubmit="enviarDatos(); return false"> -->
						    	<div class="input-group"> <label for="rol">Roles</label>
									<select class="form-control" id="rol" onchange="tipoIdentif()">
										<option>Profesor</option> <option>Ayudante</option> 
										<option selected>Estudiante</option> <option>Administrador</option>
									</select>
							    </div>
							    <br>
							    <div class="input-group">
								    <span class="input-group-addon"><i class="glyphicon glyphicon-edit"></i></span>
								    <input id="ident" type="text" class="form-control input-sm" name="ident" placeholder=" Ingrese matrícula">
							    </div>
							    <br>
						        <div class="input-group">
								    <span class="input-group-addon"><i class="glyphicon glyphicon-edit"></i></span>
								    <input id="nombre" type="text" class="form-control input-sm" name="nombre" placeholder="Nombre">
							    </div>
							    <br>
							    <div class="input-group">
								    <span class="input-group-addon"><i class="glyphicon glyphicon-edit"></i></span>
								    <input id="apellido" type="text" class="form-control input-sm" name="apellido" placeholder="Apellido">
							    </div>
							    <br>
							    <div class="input-group">
								    <span class="input-group-addon"><i class="glyphicon glyphicon-edit"></i></span>
								    <input id="carrera" type="text" class="form-control input-sm" name="carrera" placeholder="Carrera">
							    </div>
							    <br>
							    <div class="input-group">
								    <span class="input-group-addon"><i class="glyphicon glyphicon-edit"></i></span>
								    <input id="correo" type="text" class="form-control input-sm" name="correo" placeholder="Email">
							    </div>
							    <br>
							    <div class="input-group">
								    <span class="input-group-addon"><i class="glyphicon glyphicon-edit"></i></span>
								    <input id="pass" type="password" class="form-control input-sm" name="pass" placeholder="Contrasena">
							    </div>
							    <br>
						        <p id="msjMDL"></p>
						        <br>
							    <div class="form-group">
							    	<button type="btn" id="btnAdd" class="btn">Agregar</button>
							    </div>
							<!-- </form> -->
					    </div>
					</div>
				</div>		
			</div>
			<!-- Fin Modal Usr Nuevo-->

			<!-- Modal Usr Editar-->
			<div class="modal fade" id="usrEditMDL" role="dialog">
