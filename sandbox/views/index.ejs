						    <br>
						    <div class="input-group">
							    <span class="input-group-addon"><i class="glyphicon glyphicon-edit"></i></span>
							    <input id="edit_pass" type="password" class="form-control input-sm" name="pass" placeholder="Contrasena">
						    </div>
						    <br>
						    <p id="edit_rol" visible="false"></p>
						    <p id="edit_id" visible="false"></p>
						    <br>
					        <p id="msjMDL"></p>
					        <br>
						    <div class="form-group">
						    	<button type="btn" id="btnAct" class="btn" >Actualizar</button>
						    </div>
					    </div>
					</div>
				</div>		
			</div>
			<!-- Fin Modal Usr Editar-->


